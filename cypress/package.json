{
  "name": "@saucelabs/cypress-visual-plugin-example",
  "description": "Sauce Labs Visual Example for Cypress",
  "engines": {
    "node": "^16.13 || >=18"
  },
  "scripts": {
    "copy-visual": "cp ./cypress/e2e/saucedemo.visual.ts ./cypress/e2e/saucedemo.cy.ts",
    "sauce-visual": "npm run copy-visual && cypress run",
    "sauce-visual-check": "npm run copy-visual && cypress run --env VISUAL_CHECK=true"
  },
  "dependencies": {
    "@saucelabs/cypress-visual-plugin": "^0.3.325",
    "@tsconfig/node18": "^18.2.2",
    "cypress": "^13.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "chalk": "^5.3.0"
  }
}
